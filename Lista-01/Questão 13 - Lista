programa {
  inclua biblioteca Matematica --> mat
  real nota
  cadeia conceito
  funcao inicio() {
  escreva("Digite a nota: ")
  leia(nota)

          se (nota >= 0.0 e nota <= 6.0)
    {conceito = "D"}
    senao se (nota >= 6.1 e nota <= 7.5)
    {conceito = "C"}
    senao se (nota >= 7.6 e nota <= 9.0)
    {conceito = "B"}
    senao se (nota >= 9.1 e nota <= 10)
    {conceito = "A"}
    escreva("NOTA = ",mat.arredondar(nota,1), " CONCEITO = ", conceito,"\n")
  }
}
